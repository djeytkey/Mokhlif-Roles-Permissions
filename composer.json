{
    "name": "boukjijtarik/woorolemanager",
    "description": "A Laravel 12 package for WooCommerce Role & Permission Management",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Boukjij Tarik",
            "email": "tarik.engineer@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^12.0",
        "illuminate/support": "^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "BoukjijTarik\\WooRoleManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BoukjijTarik\\WooRoleManager\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BoukjijTarik\\WooRoleManager\\WooRoleManagerServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
} 